@model IEnumerable<Notic_board_2.Models.user_table>

@{
    ViewBag.Title = "StudenPanel";
    Layout = "~/Views/Shared/_LayoutHomePage.cshtml";
    ViewBag.colSize = "col-md-10";
    ViewBag.colSize2 = "col-md-0";
   
}

@section SearchBar{

    <form class="navbar-form navbar-left" role="search" action="#" method="post">
        <div class="form-group">
            <input class="form-control" id="KlassSearch" name="searchTerm" placeholder="Search" type="text">
        </div>
        <button type="submit" class="btn btn-default">Submit</button>
    </form>


}




<h2>StudenPanel</h2>
@Html.ActionLink("Create", "CreateStudent", "Admin", new { @class = "btn btn-primary", name = "Id" })
<div class="well">
    <table class="table ">
        <tr>

            <th>
                <text>ImageUrl</text>
            </th>

            <th>
                <text>FirstName</text>

             
            </th>
            <th>
                <text>LastName</text>

              
            </th>
            <th>
                <text>UserName</text>

            
            </th>
            <th>
                <text>Password</text>

             
            </th>

            <th>
                <text>RollNo</text>

            
            </th>

            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    <img class="media-object" src="~/images/dummy.jpg" width="64" height="64" alt="pic">
                </td>

                <td>
                    @item.First_name
                   
                </td>
                <td>
                    @item.Last_name
                   
                </td>
                <td>
                    @item.User_name
                 
                </td>
                <td>
                    @item.Password
                   
                </td>
                <td>
                    @item.Roll_no
                   
                </td>

                <td>
                    @Html.ActionLink("Edit", "StudentEdit", "Admin", new { @class = "btn btn-primary", name = "Id" })
                 
                    @Html.ActionLink("Delete", "StudentDelete", "Admin", new { @class = "btn btn-primary" })
                </td>
            </tr>
        }

    </table>
    </div>
